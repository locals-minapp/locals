.house-img{
    width:100%;
    height: 200px;
}
.describe{
    font-size: 0.8rem;
    margin-top: 10px;
}
.house-owner{
    border-radius: 3px;
    background-color: #e84a5f;
    color: #ffffff;
    width: 40rpx;
    height: 20rpx;
    padding:8rpx;
    line-height: 20rpx;
    text-align: center;
    margin-right: 10px;
    font-size: 24rpx;
}
.house-owner-name{
    height: 20rpx;
    line-height: 20rpx;
    font-size: 22rpx;
}
.house-owner-title{
    height: 20px;
    line-height: 20px;
    color: #333;
    font-size: 22rpx;
}
.slogan{
    display: block;
    margin-top: 5px;
    margin-bottom: 15px;
}