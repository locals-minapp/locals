/* pages/mybook/order-detail-info/cost/cost.wxss */
.container{
    background: #eeeeee;
    width: 100%;
    height: 100%;
    padding:0 0 100%;
    justify-content:stretch;
    align-items:stretch;
}
.title{
    font-size: 15px;
    height: 40px;
    display: block;
    margin: 6px 0;
    line-height: 20px;
    color:black;
    padding:0 10px;
    font-family: "arial","微软雅黑";
}

.detail{
    display: block;
    margin: 6px 0;
    line-height: 20px;
    font-size: 15px;
    height: 40px;
    color: #929699;
    padding:0 10px;
    font-family: "arial","微软雅黑";
}

.task-info{
    padding:0 10px;
    border-bottom:1px solid #ccc;
}

.left,.right{
    display: inline-block;
    margin: 6px 0;
    font-size: 14px;
    line-height: 20px;
    color: inherit;
    font-family: 'HelveticaNeue', 'Helvetica Neue', Helvetica, Arial, sans-serif;
}

.left{
    color: #929699;
}

.right{
    padding-right: 10px;
    float:right;
}

.back{
    position: fixed;
    bottom: 0;
    left:0;
    height: auto;
    width: 100%;
    text-align: center;
    border-top:1px solid #000;
    padding:10px 0;
    background-color: #fff;
}